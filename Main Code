#include <bits/stdc++.h>
using namespace std;

int main()
{
    //write your code here
    int t;
    cin >> t;
    while (t--)
    {
        int n, temp;
        cin >> n;
        while (n > 0)
        {
            temp = n;
            n = n / 10;
        }
        cout << temp << endl;
    }
    return 0;
}
